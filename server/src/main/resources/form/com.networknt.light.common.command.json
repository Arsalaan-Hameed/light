{
    "formId": "com.networknt.light.common.command",
    "version": 1,
    "action" : [{
        "title": "Submit"
    }],
    "schema": {
        "type": "object",
        "title": "Command",
        "required": ["category", "name", "readOnly"],
        "properties": {
            "category": {
                "title": "Category",
                "type": "string"
            },
            "name": {
                "title": "Name",
                "type": "string"
            },
            "readOnly": {
                "title": "ReadOnly",
                "type": "boolean"
            },
            "data": {
                "title": "Data",
                "type": "string"
            }
        }
    },
    "form": [
        "category",
        "name",
        "readOnly",
        {
            "key": "data",
            "type": "textarea"
        }
    ]
}

